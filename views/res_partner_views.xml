<odoo>
    <data>
        <record id="res_partner_add_escola_field" model="ir.ui.view">
            <field name="name">cloud_base.view_partner_form_botiga</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.view_partner_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='lang']" position="after">
                    <field name="escola" />
                    <field name="user_id"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>