<odoo>
    <data>
        <record id="cloud_product_template_form_view" model="ir.ui.view">
            <field name="name">cloud_product.template</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="product.product_template_form_view"/>
            <field name="arch" type="xml">
                <xpath expr="//header" position="inside">
                    <button name="print" string="Print" class="oe_highlight" type="object" />
                </xpath>
            </field>
        </record>
    </data>
</odoo>